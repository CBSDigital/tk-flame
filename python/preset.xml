<?xml version="1.0" encoding="UTF-8"?>
<preset version="4">
 <type>sequence</type>
 <comment>Creates 10-bit DPX and WAVE files, and a sequence in Media Library that refers to the exported media files.</comment>
 <sequence>
  <fileType>NONE</fileType>
  <namePattern>&lt;name&gt;/&lt;name&gt;</namePattern>
  <videoMedia>
   <commit>Original</commit>
   <flatten>NoChange</flatten>
   <exportHandles>1</exportHandles>
   <nbHandles>10</nbHandles>
  </videoMedia>
  <audioMedia>
   <commit>Original</commit>
   <flatten>NoChange</flatten>
   <exportHandles>1</exportHandles>
   <nbHandles>10</nbHandles>
  </audioMedia>
 </sequence>
 <video>
  <fileType>Dpx</fileType>
  <codec>923680</codec>
  <codecProfile></codecProfile>
  <namePattern>&lt;name&gt;/&lt;tape&gt;/&lt;width&gt;x&lt;height&gt;/s&lt;segment&gt;.</namePattern>
  <compressionQuality>50</compressionQuality>
  <transferCharacteristic>2</transferCharacteristic>
  <publishLinked>1</publishLinked>
  <foregroundPublish>1</foregroundPublish>
  <resize>
   <resizeType>fit</resizeType>
   <resizeFilter>lanczos</resizeFilter>
   <width>0</width>
   <height>0</height>
   <bitsPerChannel>10</bitsPerChannel>
   <numChannels>3</numChannels>
   <floatingPoint>0</floatingPoint>
   <bigEndian>1</bigEndian>
   <pixelRatio>1.000000</pixelRatio>
   <scanFormat>P</scanFormat>
  </resize>
 </video>
 <audio>
  <fileType>WAVE</fileType>
  <codec>923778</codec>
  <codecProfile></codecProfile>
  <namePattern>&lt;name&gt;/&lt;tape&gt;/audio/s&lt;segment&gt;_&lt;track&gt;</namePattern>
  <mixdown>No</mixdown>
  <sampleRate>-1</sampleRate>
  <bitRate>128</bitRate>
  <bitDepth>-1</bitDepth>
 </audio>
 <name>
  <framePadding>8</framePadding>
  <startFrame>0</startFrame>
  <useTimecode>1</useTimecode>
 </name>
 <reImport>
  <namePattern></namePattern>
 </reImport>
</preset>
